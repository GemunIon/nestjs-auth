    "@nestjs/serve-static": "2.2.2",
    "@nestjs/swagger": "5.2.1",
    "@nestjs/typeorm": "8.0.3",
    "@nestjs/websockets": "8.4.4",
    "@socket.io/redis-adapter": "7.2.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "connect-redis": "6.1.3",
    "dotenv": "16.0.0",
    "express": "4.18.1",
